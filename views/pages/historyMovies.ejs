<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>History of searched images</title>
    <link rel="stylesheet" href="/styles/adminPanel.css">
</head>
<body>
    <div class="contanier">
        <table>
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Data</th>
                    <th>Searched Date</th>
                </tr>
            </thead>
            <tbody>
                <% MovieData.forEach(function(movie) { %>
                    <tr>
                        <td><%= movie.title %></td>
                        <td>
                            <ul style="list-style: none; padding: 0; margin: 0;">
                                <% movie.data.Search.forEach(function(searchItem) { %>
                                    <li><%= searchItem.Title%> <%= searchItem.Year %></li>
                                <% }); %>
                            </ul>
                        </td>
                        <td>
                            <%= new Date(movie.date).toLocaleDateString() %>
                            <%= new Date(movie.date).toLocaleTimeString([], {hour: '2-digit', minute: '2-digit'}) %>
                        </td>
                    </tr>
                <% }); %>
            </tbody>
        </table>    
    </div>
    

</body>
</html>
